{
  "name": "@suzulabo/pages-plugin-auth",
  "version": "0.0.0",
  "description": "Cloudflare Pages Authentication Plugin",
  "homepage": "https://github.com/suzulabo/pages-plugin-auth#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/suzulabo/pages-plugin-auth.git"
  },
  "author": "Kenji Suzuki",
  "keywords": [
    "Cloudflare Pages Plugin",
    "Authentication"
  ],
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    "./google": {
      "types": "./packages/google/index.d.ts",
      "import": "./packages/google/dist/index.js"
    }
  },
  "files": [
    "./packages/google/index.d.ts",
    "./packages/google/dist/index.js",
    "./packages/google/dist/index.js.map"
  ],
  "engines": {
    "node": "16"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "test": "vitest run",
    "build": "pnpm -r run build",
    "build:watch": "pnpm -r run build:watch"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.18.0",
    "@suzulabo/code-config-typescript": "^0.0.1",
    "@tsconfig/node16-strictest": "^1.0.4",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "happy-dom": "^7.6.6",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4",
    "vitest": "^0.24.5",
    "wrangler": "^2.1.15"
  }
}
