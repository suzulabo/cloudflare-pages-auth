    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "jest": "^28.1.3",
    "prettier": "^2.6.2",
    "ts-jest": "^28.0.8",
    "typescript": "^4.8.2",
